type Character {
    characterId: String
    name: String
    gender: String
    titles: [Title]
    aliases: [Alias]
    houses: [House]
}

type Title {
    titleId: String
    title: String
}

type Alias {
    aliasId: String
    alias: String
}

type House {
    houseId: String
    name: String
    words: String
}

type Query {
    characters: [Character]
}